# Node.js and TypeScript files to ignore
node_modules
dist

# OS-generated files and others to ignore
.DS_Store
Thumbs.db
tmp

# dotenv environment variables file
.env

# Output of 'npm pack'
*.tgz

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Coverage directories used by tools like istanbul
coverage
.nyc_output

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
